import java.util.*;
class counter{
  int digitcount(int a)
  {  int c=0;
      while(a>0)
      {int d=a%10;
      c++;
      a=a/10;
      }
      return c;
  }
  public static void main(String args[])
  {counter obj=new counter();
  Scanner sc=new Scanner(System.in);
  int n=sc.nextInt();
  System.out.println(obj.digitcount(n));
      
  }
}
